{
  "name": "carwash-backend",
  "version": "1.0.0",
  "description": "Secure Car Wash Management Backend with PostgreSQL",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup:db": "node setup-db.js setup",
    "test:db": "node setup-db.js diagnostic",
    "clean:db": "node setup-db.js clean",
    "generate:hash": "node -e \"const bcrypt = require('bcrypt'); const password = process.argv[1] || 'admin123'; console.log('Password Hash:', bcrypt.hashSync(password, 10));\"",
    "generate:jwt": "node -e \"console.log('JWT Secret:', require('crypto').randomBytes(64).toString('hex'));\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "carwash",
    "management",
    "postgresql",
    "express",
    "authentication",
    "jwt"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.3",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "helmet-csp": "^4.0.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "pg": "^8.16.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
